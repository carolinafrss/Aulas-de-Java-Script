// Esse é um comentario em JavaScritpt
/*
Esse comentario usa 
varias linhas
*/

let nome = "Carolina"
let mensagem = "Hello! World"
let numero = 1200;   /*Um tipo de dado String*/
let booleano = false  /*Um tipo de dado Booleano*/
 

console.log (typeof nome);   /*Console log é onde "printa" a mesnagem na Tela*/
console.log (typeof numero); 
console.log (booleano)
console.log (mensagem);
console.log ("Hello! World!")
console.log (typeof booleano);

